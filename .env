# ============================================
# Yao Application Configuration
# ============================================

YAO_DB_DRIVER="sqlite3"                     # Database driver: sqlite3, mysql, postgres
YAO_DB_PRIMARY="./db/yao.db"                # Primary database connection string
YAO_ENV="development"                        # Environment: development, production
YAO_HOST="0.0.0.0"                          # Server host address
YAO_PORT=5099                               # Server port
YAO_LOG="./logs/application.log"            # Log file path
YAO_LOG_MODE="TEXT"                         # Log mode: TEXT, JSON
YAO_SESSION_FILE="./db/.session"            # Session file path
YAO_SESSION_STORE="file"                    # Session store type: file, redis


# ============================================
# OpenAPI / OAuth Configuration
# ============================================

YAO_CLIENT_ID="uZHHcfDTTkXaPDREwgXuCtSsqC7c2ivT"    # OAuth client ID (auto-generated if not set)
# YAO_CLIENT_SECRET=""                              # OAuth client secret (optional, for backend services)


# ============================================
# Authentication Configuration
# ============================================

AFTER_LOGIN_SUCCESS_URL="/dashboard/chat"           # Redirect URL after successful login
AFTER_LOGIN_FAILURE_URL="/dashboard/auth/entry"     # Redirect URL after failed login

# Application Base URL (for email links, invitations, etc.)
# APP_BASE_URL="<https://your-domain.com>"            # e.g., https://example.com (no trailing slash)


# ============================================
# Default LLM Configuration
# ============================================
# Set the default LLM connector for Agent. You must configure at least one:
#   - A cloud LLM API key (see "LLM API Keys" sections below), OR
#   - A local LLM via Ollama (see "Local LLM - Ollama" section)
# Format: <provider>.<model>, e.g., openai.gpt-4o, ollama.llama3_3
# Available connectors are defined in /connectors/<provider>/*.conn.yao

DEFAULT_LLM="volcengine.deepseek-v3"


# ============================================
# LLM API Keys - Native Providers
# ============================================

# OpenAI (gpt-4o, gpt-4o-mini, gpt-5.2, o3, text-embedding-3-large)
# OPENAI_API_KEY="<your-openai-api-key>"

# Anthropic (claude-opus-4.5, claude-sonnet-4.5)
# ANTHROPIC_API_KEY="<your-anthropic-api-key>"

# Google (gemini-2.5-pro, gemini-3-flash)
# GOOGLE_API_KEY="<your-google-api-key>"

# xAI (grok-4)
# XAI_API_KEY="<your-xai-api-key>"

# DeepSeek (deepseek-chat, deepseek-reasoner)
# DEEPSEEK_API_KEY="<your-deepseek-api-key>"

# Meta Llama (llama-4-maverick)
# META_API_KEY="<your-meta-api-key>"

# Mistral (mistral-large-3)
# MISTRAL_API_KEY="<your-mistral-api-key>"


# ============================================
# Local LLM - Ollama
# ============================================

# Ollama (llama3.3, deepseek-r1, qwen3, gemma3, qwen2.5-coder) - Local inference
# OLLAMA_HOST="http://127.0.0.1:11434/v1"


# ============================================
# LLM API Keys - MaaS Platforms
# ============================================

# Azure OpenAI (gpt-5.2)
# AZURE_OPENAI_API_KEY="<your-azure-openai-api-key>"
# AZURE_OPENAI_ENDPOINT="<https://your-resource.openai.azure.com/openai/v1>"

# Groq (llama-4-maverick) - Fast inference
# GROQ_API_KEY="<your-groq-api-key>"

# Together AI (llama-4-maverick, deepseek-r1)
# TOGETHER_API_KEY="<your-together-api-key>"

# Fireworks AI (llama-4-maverick)
# FIREWORKS_API_KEY="<your-fireworks-api-key>"

# OpenRouter (auto, claude-opus-4.5, nova-premier) - Multi-provider gateway
# OPENROUTER_API_KEY="<your-openrouter-api-key>"

# SiliconFlow (deepseek-v3, qwen-2.5-72b) - China MaaS
# SILICONFLOW_API_KEY="<your-siliconflow-api-key>"

# Volcengine / ByteDance Ark (doubao-1.5-pro, deepseek-v3, deepseek-r1, glm-4-plus, qwen-vl-max) - China MaaS
# VOLCENGINE_API_KEY="<your-volcengine-api-key>"


# ============================================
# Web Search API (for Agent search capabilities)
# ============================================

# Serper (Google Search API) - https://serper.dev
# SERPER_API_KEY="<your-serper-api-key>"


# ============================================
# Messaging Services (Email / SMS / WhatsApp)
# ============================================

# Mailgun - Transactional Email - https://app.mailgun.com/app/sending/domains
# MAILGUN_DOMAIN="<your-mailgun-domain>"                  # e.g., mg.example.com
# MAILGUN_API_KEY="<your-mailgun-api-key>"                # Starts with "key-"
# MAILGUN_FROM="<noreply@your-domain.com>"                # Sender email address

# AI Member Email Domains (for robot/staff email addresses)
# ROBOT_EMAIL_DOMAIN="<ai.your-domain.com>"               # e.g., ai.example.com
# STAFF_EMAIL_DOMAIN="<staff.your-domain.com>"            # e.g., staff.example.com

# Twilio - SMS / WhatsApp / SendGrid - https://console.twilio.com
# TWILIO_ACCOUNT_SID="<your-twilio-account-sid>"          # Starts with "AC"
# TWILIO_AUTH_TOKEN="<your-twilio-auth-token>"
# TWILIO_FROM_PHONE="<+1234567890>"                       # Twilio phone number for SMS
# TWILIO_FROM_EMAIL="<noreply@your-domain.com>"           # SendGrid sender email
# TWILIO_API_SID="<your-twilio-api-sid>"                  # Optional: API Key SID
# TWILIO_API_KEY="<your-twilio-api-key>"                  # Optional: API Key Secret
# TWILIO_SENDGRID_API_KEY="<your-sendgrid-api-key>"       # Optional: SendGrid API key


# ============================================
# Third-Party OAuth Providers (Optional)
# ============================================

# GitHub OAuth - https://github.com/settings/developers
# GITHUBUSER_CLIENT_ID="<your-github-client-id>"
# GITHUBUSER_CLIENT_SECRET="<your-github-client-secret>"

# Google OAuth - https://console.cloud.google.com/apis/credentials
# GOOGLE_CLIENT_ID="<your-google-client-id>"
# GOOGLE_CLIENT_SECRET="<your-google-client-secret>"

# Apple Sign In - https://developer.apple.com/account/resources/identifiers
# APPLE_SERVICE_ID="<your-apple-service-id>"
# APPLE_TEAM_ID="<your-apple-team-id>"
# APPLE_KEY_ID="<your-apple-key-id>"
# APPLE_PRIVATE_KEY_PATH="<apple-authkey.p8>"               # Relative to openapi/certs/

# Microsoft OAuth - https://portal.azure.com/#blade/Microsoft_AAD_RegisteredApps
# MICROSOFT_CLIENT_ID="<your-microsoft-client-id>"
# MICROSOFT_CLIENT_SECRET="<your-microsoft-client-secret>"
